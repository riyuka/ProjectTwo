<div class = "accountLogInDiv">
    <div class = "loginLinks">
        <% if(user) { %>
            <p>Welcome! <%= user %></p>
            <a href="/myaccount" class="btn btn-outline-success" role="button" aria-disabled="true">My Account</a>
            <a href="/logout" class="btn btn-outline-secondary" role="button" aria-disabled="true">Log Out</a>
        <% }else{ %>
            <a href="/login" class="btn btn-outline-success" role="button" aria-disabled="true">Sign in</a>
            
            <a href="/signup" class="btn btn-outline-secondary" role="button" aria-disabled="true">Sign Up</a>
        <% } %>
    </div>
    
</div>